<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local = "clr-namespace:Wanderlust"
            x:Class="Wanderlust.HomePage"
            SelectedTabColor="Brown"
            Title="Wanderlust"
            BarTextColor="Brown"
            >


    <TabbedPage.ToolbarItems>
        <ToolbarItem Text="Add"
                     Icon= "add.png"
                     Clicked="ToolbarItem_Clicked"
                      
                     />
    </TabbedPage.ToolbarItems>
    <local:ProfilePage Title="Profile"
                       Icon ="user.png"/>

    <local:HistoryPage Title="History"
                       Icon="history.png"/>
    <local:MapPage Title="Map"
                   Icon="map.png"/>
    <local:ExpolererPage Title="Explorer"
                   Icon="search.png"/>
    

</TabbedPage>
